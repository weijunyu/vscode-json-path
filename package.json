{
  "name": "vscode-json-path",
  "version": "2.4.0",
  "description": "Extract and find data in JSON structures with JSONPath queries",
  "repository": {
    "type": "git",
    "url": "https://github.com/weijunyu/vscode-json-path.git"
  },
  "license": "MIT",
  "author": {
    "name": "Wei Junyu",
    "email": "junyuwei@gmail.com"
  },
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "jsonpath": "^1.0.2",
    "worker-farm": "^1.7.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.17.28",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7",
    "vscode-test": "^1.4.0"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "activationEvents": [
    "onCommand:extension.jsonPath",
    "onCommand:extension.jsonPathWithNodes"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.jsonPath",
        "title": "jsonpath: Extract JSON data"
      },
      {
        "command": "extension.jsonPathWithNodes",
        "title": "jsonpath: Extract JSON data with their paths"
      }
    ]
  },
  "displayName": "JSON Path",
  "icon": "icon.png",
  "publisher": "weijunyu"
}
